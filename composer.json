{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.8",
        "endroid/qr-code-bundle": "^4.0",
        "league/html-to-markdown": "^5.0",
        "sensio/framework-extra-bundle": "^6.1",
        "stof/doctrine-extensions-bundle": "^1.6",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/http-foundation": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/mailjet-mailer": "6.4.*",
        "symfony/maker-bundle": "^1.31",
        "symfony/monolog-bundle": "^3.7",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/security-core": "6.4.*",
        "symfony/security-csrf": "6.4.*",
        "symfony/security-http": "6.4.*",
        "symfony/stimulus-bundle": "^2.19",
        "symfony/translation": "^6.4",
        "symfony/twig-bundle": "6.4.*",
        "symfony/ux-icons": "^2.23",
        "symfony/ux-twig-component": "^2.19",
        "symfony/validator": "6.4.*",
        "symfony/var-dumper": "6.4.*",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "6.4.*",
        "symfonycasts/reset-password-bundle": "^1.22",
        "symfonycasts/tailwind-bundle": "^0.6.0",
        "symfonycasts/verify-email-bundle": "^1.17",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "3.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/stopwatch": "^5.3",
        "symfony/web-profiler-bundle": "6.4.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
